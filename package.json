{
  "name": "ts-to-json",
  "version": "1.0.0",
  "description": "Parse TypeScript interfaces as JSON",
  "main": "ts-to-json",
  "bin": {
    "ts-to-json": "bin/ts-to-json"
  },
  "dependencies": {
    "micromatch": "^3.1.10",
    "ora": "^3.0.0",
    "ts-node": "^7.0.1",
    "ts-simple-ast": "^20.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "ava": "^1.0.1"
  },
  "scripts": {
    "start": "tsc && ts-node ./bin/ts-to-json",
    "test": "npx ava --watch"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "author": "Johan Olofsson",
  "license": "MIT"
}
